{
  "name": "@luvies/cors-proxy",
  "version": "0.1.1",
  "description": "A simple CORS proxy CLI",
  "main": "dist/lib/index.js",
  "bin": {
    "cors-proxy": "dist/bin/cli.js"
  },
  "repository": "https://github.com/luvies/cors-proxy",
  "author": "luvies",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/* *.tsbuildinfo",
    "test": "true",
    "lint": "./node_modules/@luvies/config/scripts/lint.sh",
    "fix": "./node_modules/@luvies/config/scripts/lint.sh fix",
    "prepare": "yarn clean && yarn build && yarn lint"
  },
  "devDependencies": {
    "@luvies/config": "^2.0.3",
    "@types/koa": "^2.0.49",
    "@types/node": "^12.7.5",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.2",
    "koa": "^2.8.1",
    "node-fetch": "^3.1.1",
    "raw-body": "^2.4.1",
    "yargs": "^14.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md",
    "yarn.lock"
  ]
